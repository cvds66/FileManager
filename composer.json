{
    "name": "webplusmultimedia/filemanager",
    "description": "Manage files with this file manager made with alpinejs and livewire and tailwindcss",
    "keywords": [
        "webplusmultimedia",
        "laravel",
        "filemanager"
    ],
    "homepage": "https://github.com/webplusmultimedia/filemanager",
    "license": "MIT",
    "authors": [
        {
            "name": "webplusm",
            "email": "contact@webplusm.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "livewire/livewire": "^2.12",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webplusmultimedia\\FileManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webplusmultimedia\\FileManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webplusmultimedia\\FileManager\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "Webplusmultimedia\\FileManager\\Facades\\FileManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
